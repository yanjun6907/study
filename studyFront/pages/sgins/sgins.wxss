
  page{
    height: 100%;
  }  
  .container{
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    position: relative;
  }
  .image{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: -100;
  }
  .main {
    background-color: #fff;
  }
  .topBar {
    height: 40rpx;
    background-color: #038a94;
  }
  .topBarTitle {
    text-align: right;
    padding-right: 10rpx;
    height: 80rpx;
    line-height: 80rpx; 
    font-size: 30rpx;
  }
  .calendar-background {
    font-size: 35rpx;
  }
  
  .calendar-item {
    width: 8%;
    height: 120rpx;
    line-height: 120rpx;
    padding: 5rpx 3.13%;
    text-align: center;
    font-size: 28rpx;
    font-weight: 550;
    border-bottom: 1px solid #c7c7c7;
  }
  .calendar-item1 {
    width: 8%;
    height: 70rpx;
    line-height: 70rpx;
    padding: 5rpx 3.1%;
    text-align: center;
    font-size: 28rpx;
    font-weight: 550;
  }
  .display-space-between1 {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
  }
  .display-space-between2 {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx; 
    text-align: center;
    background-color: rgb(235, 235, 235);
    border: 1px solid #c7c7c7;
  }
  .text-gray {
    color: #ffffff;
  }
  .text-red {
    background-color: #F87474;
  }
  .btn {
    padding: 20rpx 50rpx;
  }
  .green {
    color: #fff;
    background-color: #00c700;
  }
 
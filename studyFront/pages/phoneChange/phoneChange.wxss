.weui-cells {
  margin-top: 0;
}
.weui-cell {
  height: 100rpx;
  padding: 0 0 0 20rpx;
  margin: 30rpx 0;
  border: 1px solid rgb(224, 224, 224);
  font-size: 32rpx;
}
.weui-cell2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 180rpx;
  padding: 0 0 0 20rpx;
  margin: 30rpx 0;
  border: 1px solid rgb(224, 224, 224);
  font-size: 32rpx;
}
.weui-label {
  display: block;
  width: 120rpx;
}
.weui-btn-area {
  margin: 0;
}
.weui-btn {
  position: relative;
  display: block;
  width: 90%;
  background-color: #0077ffe8;
}
.weui-vcode-btn {
  display: inline-block;
  padding: 0 .6em 0 .7em;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #0077ffe8;
  position: relative;
}
.weui-vcode-btn, .weui-vcode-img {
  margin-left: 5px;
  height: 100rpx;
  vertical-align: middle;
}